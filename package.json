{
  "name": "yb",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "7.1.0",
    "@svgr/webpack": "2.4.1",
    "aes256": "1.0.4",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "9.0.0",
    "babel-jest": "23.6.0",
    "babel-plugin-named-asset-import": "0.2.3",
    "babel-plugin-react-css-modules": "3.4.2",
    "babel-preset-react-app": "5.0.4",
    "bfj": "6.1.1",
    "case-sensitive-paths-webpack-plugin": "2.1.2",
    "chalk": "2.4.1",
    "classnames": "2.2.6",
    "event-stream": "3.3.5",
    "file-loader": "2.0.0",
    "fs-extra": "7.0.0",
    "gulp": "^4.0.2",
    "html-webpack-plugin": "4.0.0-alpha.2",
    "identity-obj-proxy": "3.0.0",
    "js-yaml": "3.2.7",
    "jwt-decode": "2.2.0",
    "nouislider-react": "3.3.1",
    "optimize-css-assets-webpack-plugin": "5.0.1",
    "pnp-webpack-plugin": "1.1.0",
    "prop-types": "15.7.2",
    "resolve": "1.8.1",
    "stubby": "0.3.1",
    "style-loader": "0.23.1",
    "supertest": "0.7.1",
    "url-loader": "1.1.1",
    "vinyl-fs": "0.3.14",
    "workbox-webpack-plugin": "3.6.2"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "sonarqube": "sonarqube-scanner-node -Dsonar.host.url=$url -Dsonar.login=$token",
    "build": "node scripts/build.js",
    "fmt": "node ./src/jeans/tasks/taskRunner.js -c fmt",
    "lint": "node ./src/jeans/tasks/taskRunner.js -c lint",
    "test": "node ./src/jeans/tasks/taskRunner.js -c test"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "eslint-config-postcss"
    ],
    "rules": {
      "import/no-unresolved": 0
    }
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "lcov",
      "html"
    ],
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "src/**/**/*.{js,jsx}",
      "!src/jeans/bin/generateJeansFiles/**",
      "!src/utils/portfolioIcons/**"
    ],
    "testResultsProcessor": "jest-sonar-reporter",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "resolver": "jest-pnp-resolver",
    "setupFiles": [
      "react-app-polyfill/jsdom",
      "<rootDir>/src/setupTests.js",
      "jest-localstorage-mock"
    ],
    "testMatch": [
      "<rootDir>/src/**/?(*.)(spec|test).{js,jsx}",
      "<rootDir>/src/**/**/?(*.)(spec|test).{js,jsx}"
    ],
    "testEnvironment": "jsdom",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.css$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|css|json)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$",
      "^.+\\.module\\.(css|sass|scss)$"
    ],
    "moduleNameMapper": {
      "^react-native$": "react-native-web",
      "^.+\\.module\\.(css|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "web.js",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ]
  },
  "babel": {
    "presets": [
      "react-app"
    ]
  },
  "devDependencies": {
    "babel-loader": "8.0.6",
    "glob-all": "^3.1.0",
    "gulp-jshint": "^1.12.0",
    "gulp-nodeunit": "^0.0.5",
    "gulp-stubby-server": "^0.2.1",
    "gulp-util": "3.0.8",
    "jest-localstorage-mock": "2.4.0",
    "jshint": "^2.10.2",
    "terser-webpack-plugin": "1.1.0",
    "webpack": "4.19.1",
    "webpack-dev-server": "3.1.9",
    "lodash": "^4.17.14",
    "webpack-manifest-plugin": "2.0.4"
  }
}